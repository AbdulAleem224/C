#include <stdio.h>

int main() 
{
    int sum=0;
    int n;
    printf("enter number how many time repeated");
    scanf("%d",&n);
    for(int a=1;a<=n;a++)
  
    {
          sum=sum+a;
     
       
    }
      printf("sum is %d \n",sum);
    return 0;
}

