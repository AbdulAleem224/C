//postincrement
//30 Aug 2022
#include <stdio.h>

void main() {
    // dicrement oprater
    int i;
    i=15;
    printf("value of i=%d\n",i);
    i--;
    printf("value of i=%d\n",i);
     printf("--------------\n");

    int  x;
    x=i++;
    printf("value of i=%d\n",i);
    printf("value of x=%d",x);
    
    
}
