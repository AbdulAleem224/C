// write a program to swap two number using call by refrence.
#include <stdio.h
void swap()
{
   { 
   int *x;
   int *y;
    int temp;
    temp=*x;
    *x=*y;
    *y=temp;
}
   void main ()
   {
       int a=20;
       int b=60;
       printf("before swap a=%d b=%d",a,b);
       swap(&a,&b);
       printf("after swap a=%d b=%d",a,b);
   }
}
